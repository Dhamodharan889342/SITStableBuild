<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Time_Taken__c</fullName>
    <description>Created By - Mayank Pant
Created Date - 27th Nov 2020
User Story - https://jira.hyundai-autoever.eu/browse/GD-1448
Purpose -  This field will show the time taken</description>
    <externalId>false</externalId>
    <formula>ROUND(MOD((EndTime__c - StartTime__c)*24,24),2)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Time Taken (Hrs)</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackHistory>false</trackHistory>
    <trackTrending>false</trackTrending>
    <type>Number</type>
    <unique>false</unique>
</CustomField>
